{
  "languageserver": {
    "elixirLS": {
      "command": "/Users/rwh02/.elixir/language-server/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "rust": {
      "command": "rls",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    },
    "golang": {
      "command": "gopls",
      "filetypes": ["go"]
    },
    "kotlin": {
      "command": "~/.kotlin/language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"]
    }
  },
  "suggest.noselect": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "rust-client.rlsPath": "rls",
  "rust-client.disableRustup": true,
  "yaml.format.enable":true,
  "yaml.trace.server":"verbose",
  "yaml.schemaStore.enable": false,
  "yaml.schemas": {
      "kubernetes": "/templates/**/*.{yaml,yml}",
      "/Users/rwh02/Documents/Code/Golden-Pants/build-a-thing/poypoy/resources/schemas/deployment.json": "*/.rip/deploy.yaml"
  }
}
